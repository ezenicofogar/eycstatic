{
  "name": "eycstatic",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:css": "pnpx @tailwindcss/cli -i ./tailwind.input.css -o ./static/css/tailwind.css --watch",
    "build:css": "pnpx @tailwindcss/cli -i ./tailwind.input.css -o ./static/css/tailwind.css --minify",
    "watch:11ty": "pnpx @11ty/eleventy --serve",
    "build:11ty": "pnpx @11ty/eleventy",
    "watch": "concurrently --names \"TW,11TY\" \"pnpm run watch:css\" \"pnpm run watch:11ty\"",
    "build": "pnpm run build:css && pnpm run build:11ty"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/cli": "^4.1.17",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.17"
  },
  "type": "module"
}
